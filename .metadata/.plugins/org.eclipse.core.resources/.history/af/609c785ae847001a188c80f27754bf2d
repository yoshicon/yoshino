package javabook.c6;

import java.util.StringTokenizer;

public class Ch6Ex8 {
	public static void main(String[] args) {
		String m = "맘스터치, 롯데리아, 본죽, 돈까스, 순대국, 홍콩반점, 덮밥, 초밥";
		StringTokenizer st = new StringTokenizer(m, ", ");

		System.out.println("믜 : "+m.nextToken());
		System.out.println("믜 : "+m.nextToken());
		System.out.println("믜 : "+m.nextToken());
		System.out.println("믜 : "+m.nextToken());
		System.out.println("믜 : "+m.nextToken());
		System.out.println("믜 : "+m.nextToken());
		System.out.println("믜 : "+m.nextToken());
		System.out.println("믜 : "+m.nextToken());
	}

}
